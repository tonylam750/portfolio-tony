#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tonyl/dev/tonys-bar/node_modules/.pnpm/opentype.js@1.3.4/node_modules/opentype.js/bin/node_modules:/Users/tonyl/dev/tonys-bar/node_modules/.pnpm/opentype.js@1.3.4/node_modules/opentype.js/node_modules:/Users/tonyl/dev/tonys-bar/node_modules/.pnpm/opentype.js@1.3.4/node_modules:/Users/tonyl/dev/tonys-bar/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tonyl/dev/tonys-bar/node_modules/.pnpm/opentype.js@1.3.4/node_modules/opentype.js/bin/node_modules:/Users/tonyl/dev/tonys-bar/node_modules/.pnpm/opentype.js@1.3.4/node_modules/opentype.js/node_modules:/Users/tonyl/dev/tonys-bar/node_modules/.pnpm/opentype.js@1.3.4/node_modules:/Users/tonyl/dev/tonys-bar/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../opentype.js@1.3.4/node_modules/opentype.js/bin/ot" "$@"
else
  exec node  "$basedir/../../../../../opentype.js@1.3.4/node_modules/opentype.js/bin/ot" "$@"
fi
